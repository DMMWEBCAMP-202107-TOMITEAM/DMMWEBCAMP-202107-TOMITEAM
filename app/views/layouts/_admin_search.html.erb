<div class="search_form">
  <%= form_with(url: admin_search_path,method: :get, local: true) do |f| %>
    <%= f.text_field :content,placeholder:"Search", class:"search_form_text" %>
    <%= f.select :model,options_for_select({"商品"=>"item","顧客"=>"client"})%>
    <%= f.button :type => "submit", class:"search_form_btn" do %>
      <i class="fas fa-search search_form_icon"></i>
    <% end %>
  <% end %>
</div>