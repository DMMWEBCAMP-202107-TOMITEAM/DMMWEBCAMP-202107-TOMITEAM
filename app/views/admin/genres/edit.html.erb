<div class='container'>
  <h2>ジャンル編集</h2>
    <div class='row'>
     <div class="col-6">
      <div class="form-group col-10">
        <%= form_with model:@genre, url: '/admins/genre', local:true do |f| %>
         <%= f.label :ジャンル名 %>
         <%= f.text_field :name,class: "col-4" %>
         <%= f.submit "変更を保存", class: "glyphicon glyphicon-pencil btn btn-success pull-right" %>
         <% end %>
        </div>
      </div>
    </div>
 </div>